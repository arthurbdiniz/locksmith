<header>
  <div>
    <nav>
      <ul>
        <% if logged_in? %>
          <li>
            <ul>
              <li><%= link_to "Home", current_user %></li>
              <li><%= link_to "Settings", edit_user_path(current_user) %></li>
              <li><%= link_to "Documents", documents_path %></li>
              <li>
                <%= link_to "Log out", sign_out_path, method: "delete" %>
              </li>
            </ul>
          </li>
        <% else %>
          <li><%= link_to "Log in", root_path %></li>
        <% end %>
        

      </ul>
    </nav>
  </div>
</header>